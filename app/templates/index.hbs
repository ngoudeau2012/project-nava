<MainContainer>
    <AddDog />
    <Row> 
    {{#each this.model as |dog| }}
    <LinkTo @route="dog" @model={{dog}} class="col-3">
        <DogCard 
        @name={{dog.name}}
        @owner={{dog.owner}}
        @breed={{dog.breed}}
        @size={{dog.size}}
        @description={{dog.description}}
        />
    </LinkTo>
    {{/each}}
    </Row>
</MainContainer>

{{outlet}}