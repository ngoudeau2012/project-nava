<MainContainer>
    <AddDog />
    {{input type="text" id="myInput" value=this.filter}}
    <Row> 
    {{#each this.model as |dog| }}
        <DogCard
        @dog={{dog}}
        @id={{dog.id}}
        @name={{dog.name}}
        @owner={{dog.owner}}
        @breed={{dog.breed}}
        @size={{dog.size}}
        @description={{dog.description}}
        />
    {{/each}}
    </Row>
</MainContainer>

{{outlet}}